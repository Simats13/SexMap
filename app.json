{
  "expo": {
    "name": "SexMap",
    "slug": "sexmap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sexmap",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "dark",
      "bundleIdentifier": "com.sexmap.off",
      "associatedDomains": ["applinks:sexmap.com"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sexmap.off",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "sexmap",
              "host": "sexmap.com",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", ["expo-notifications"], [
      "expo-location",
      {
        "locationAlwaysAndWhenInUsePermission": "Permettez à $(PRODUCT_NAME) d'utiliser votre position pour afficher les points d'intérêt près de vous."
      }
    ]],
    "experiments": {
      "typedRoutes": true
    },
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "barStyle": "light-content"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f90e4201-63d6-4f34-8704-b78e6765c2ad"
      }
    },
    "owner": "simats"
  }
}
